#test name: basic-ftp
url --url=@KSTEST_FTP_URL@
install
network --bootproto=dhcp

bootloader --timeout=1
zerombr
clearpart --all
autopart

keyboard us
lang en
timezone America/New_York --nontp
rootpw qweqwe
shutdown

%packages
%end

%post
# If we made it post, that's good enough
echo SUCCESS > /root/RESULT
%end
